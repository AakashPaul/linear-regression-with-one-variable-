   "source": [
    "#Linear Regression with One Variable\n",
    "#importing necessary library files\n",
    "import numpy as np\n",
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 18,
   "metadata": {},
   "outputs": [],
   "source": [
    "#loading the data file  \n",
    "data=pd.read_csv(\"C:\\\\Octave\\\\ex1.txt\",header=None)"
    "data.load()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 19,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "\n",
      "text/plain": [
       "<Figure size 432x288 with 1 Axes>"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "#plotting the given data\n",
    "x=data.iloc[:,0] \n",
    "y=data.iloc[:,1]\n",
    "m=len(y)\n",
    "plt.scatter(x, y)\n",
    "plt.xlabel('Population of City in 10,000s')\n",
    "plt.ylabel('Profit in $10,000s')\n",
    "plt.show()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 20,
   "metadata": {},
   "outputs": [],
   "source": [
    "#setting the parameters\n",
    "x=x[:,np.newaxis]\n",
    "y=y[:,np.newaxis]\n",
    "theta=np.zeros([2,1])\n",
    "iterations=1500\n",
    "alpha=0.01\n",
    "ones = np.ones((m,1))\n",
    "x=np.hstack((ones,x))\n",
    "m=len(y)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 21,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "32.072733877455676\n"
     ]
    }
   ],
   "source": [
    "#defining cost function\n",
    "def computeCost(x,y,theta):\n",
    "    t=np.dot(x,theta)-y\n",
    "    return np.sum(np.power(t,2))/(2*m)\n",
    "j=computeCost(x,y,theta)\n",
    "print(j)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 22,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[[-3.63029144]\n",
      " [ 1.16636235]]\n"
     ]
    }
   ],
   "source": [
    "#defining gradient descent function and finding the new theta\n",
    "def gd(x,y,theta,alpha,iterations):\n",
    "    for _ in range(iterations):\n",
    "        temp = np.dot(x, theta) - y\n",
    "        temp = np.dot(x.T, temp)\n",
    "        theta = theta - (alpha/m) * temp\n",
    "    return theta\n",
    "theta1= gd(x, y, theta, alpha, iterations)\n",
    "print(theta1)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 23,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "4.483388256587726\n"
     ]
    }
   ],
   "source": [
    "#checking the new value of cost function\n",
    "j=computeCost(x,y,theta1)\n",
    "print(j)"
   ]
  },
